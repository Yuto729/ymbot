{
  "name": "ymbot",
  "version": "1.0.0",
  "description": "Autonomous AI agent powered by Claude Agent SDK",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "lefthook install"
  },
  "keywords": [
    "ai",
    "agent",
    "claude",
    "autonomous"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.34"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^25.2.1",
    "@vitest/ui": "^4.0.18",
    "lefthook": "^2.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
